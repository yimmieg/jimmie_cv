@article{peirce_replicable_2006,
author = {Jeremy Peirce and Hongqiang Li and Jintao Wang and Kenneth Manly and Robert Hitzemann and John Belknap and Glenn Rosen and Shirlean Goodwin and Thomas Sutter and Robert Williams and Lu Lu}, 
journal = {Mammalian Genome},
title = {How replicable are mRNA expression QTL?},
abstract = {Abstract Applying quantitative trait analysis methods to genome-wide microarray-derived mRNA expression phenotypes in segregating populations is a valuable tool in the attempt to link high-level traits to their molecular causes. The massive multiple-testing issues involved in analyzing these data make the correct level of confidence to place in mRNA abundance quantitative trait loci (QTL) a difficult problem. We use a unique resource to directly test mRNA abundance QTL replicability in mice: paired recombinant inbred (RI) and F2 data sets derived from C57BL/6J (B6) and DBA/2J (D2) inbred strains and phenotyped using the same Affymetrix arrays. We have one forebrain and one striatum data set pair. We describe QTL replication at varying stringencies in these data. For instance, 78% of mRNA expression QTL (eQTL) with genome-wide adjusted p ≤ 0.0001 in RI data replicate at a genome-wide adjusted p \textbackslashtextless 0.05 or better. Replicated QTL are disproportionately putatively cis-acting, and approximately 75% have higher apparent expression levels associated with B6 genotypes, which may be partly due to probe set generation using B6 sequence. Finally, we note that while trans-acting QTL do not replicate well between data sets in general, at least one cluster of trans-acting QTL on distal Chr 1 is notably preserved between data sets.},
