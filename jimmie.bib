pages = {1886--1894},
volume = {22},
year = {2006},
date-added = {2009-02-06 21:55:50 -0800},
date-modified = {2009-02-06 21:55:52 -0800},
doi = {10.1093/bioinformatics/btl279},
URL = {http://bioinformatics.oxfordjournals.org/cgi/content/abstract/22/15/1886},
uri = {papers://22BCB17C-10AB-45F2-84CB-6F2DC3D34024/Paper/p10},
