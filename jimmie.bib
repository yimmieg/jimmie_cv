date-modified = {2009-02-06 21:55:52 -0800},
doi = {10.1093/bioinformatics/btm327},
URL = {http://bioinformatics.oxfordjournals.org/cgi/content/abstract/23/17/2290},
uri = {papers://22BCB17C-10AB-45F2-84CB-6F2DC3D34024/Paper/p14},
